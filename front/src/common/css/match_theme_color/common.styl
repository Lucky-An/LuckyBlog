.el-button{
  background:none!important;
  border-color:#323659!important;
  color:#abadba!important;
}
.el-button--primary {
  color: #fff!important;
  background-color: #3074fd!important;
  border-color: #3074fd!important;
}
.positionAB{
  position:absolute!important;
  opacity:0;
  z-index:-2;
}
.solving_type {
  background-image: url('~@/assets/img/ad5/bj1.jpg');
  background-repeat:no-repeat;
  background-size:100% 100%;
  background-attachment fixed;
  background-position left top;
  min-height:100%;
  .header {
    width:calc(100% - 488px)!important;
    background: url('~@/assets/img/ad5/competition/jst2.png') repeat-x  !important;
    border-bottom: none !important;
    height: 60px !important;
    padding:0 80px 0 0!important;
    float:right;
    position: relative;
    z-index: 1866;
    .ad_logo{
      width:488px!important;
      height:83px!important;
      margin-top:0!important;
      background:url('~@/assets/img/ad5/competition/jst1.png') no-repeat;
      background-size:100% 100%;
      position:absolute;
      left:-488px;
      top:0;
      img{
        width:360px!important;
        height:80px!important;
        max-width:480px!important;
        max-height:80px!important;
      }
    }
    .user_handle .user_info .user_detail .user_logo {
      border: 2px solid light_blue !important;
    }

    .ad_logo {
      margin-right: 0 !important;
    }

    .tab_list {
      display: inline-block;
      margin-left:-12%;
      .tab-item {
        width: auto !important;
        height: 62px !important;
        line-height: 62px !important;

        a {
          padding-left: 20px;
          padding-right: 20px;
          line-height: 68px;
          height: 60px;
        }
        background: none !important;

        .tab-active {
          background: none !important;
          color:lightblue!important;
          font-weight:bold;
          border-bottom:4px solid lightblue;
        }
      }
    }
    .user_handle{
      margin-top:10px!important;
    }
    .user_detail{
      height:52px;
    }
    .handle_box {
      top: 50px !important;
      background: #1e2252;
      padding: 10px 0;
      ul li{
        background:#1e2252!important;
        border-bottom:none!important;
        &:hover{
          background:lightblue!important;
          a,span{
            color:#fff!important;
          }
        }
        a,span{
          color:#abaebd!important;
        }
      }
    }
  }
  .logoIsCenter{
    text-align:center;
    .logoCenter{
      width:100%!important;
      padding-right:0!important;
      background: url('~@/assets/img/ad5/tou.png') no-repeat!important;
      background-size:100% 100%!important;
      .ad_logo{
        margin-top:0!important;
        background:none;
        background-size:100% 100%;
        position:absolute;
        left:0;
        right:0;
        top:0;
        margin:auto;
        margin-right:auto!important;
        img{
          max-width:460px!important;
          max-height:90px!important;
        }
      }
      .user_handle{
        position:absolute;
        top:0;
        right:0;
      }
      .tab_list{
        margin-left:0;
        .tab-item{
          a{
            color:#fff;
          }
          &:first-child{
            margin-left:0;
          }
          &:last-child{
            margin-right:0;
          }
        }
      }
    }
  }
  .match_top .match_title {
    border-bottom: 1px solid light_blue !important;
  }

  .guide_wrap .list .guide_title .title_index span {
    color: #122430;
  }

  .guide_wrap ul.i_am_notice {
    padding: 0 30px;

    li {
      background: #1b1f3c !important;
      border: none !important;
      border-radius 5px;
      padding: 10px 20px;
      .notice_time {
        border-bottom: none;
        color: #626584;
        font-size: 14px;
      }
    }
  }
  .left_side{
    width:calc(100% - 280px)!important;
  }
  .left_side{
    float:left;
  }
  .trend_board{
    float:right;
  }
  .trend_board{
    width:250px;
    height:377px;
    background:panel_bg!important;
    border-radius 8px;
    .trend_data_title{
      background:#272a49!important;
      line-height:54px;
      color:#fff;
      font-size:16px;
      border-top-left-radius 8px;
      border-top-right-radius 8px;
    }
    .trend_data_body{
      padding:6px 10px 0;
      ul{
        li{
          line-height:58px;
          border-bottom:1px solid rgba(255,255,255,.15);
          text-align:left;
          color:#fff;
          font-size:16px;
          padding-left:20px;
          &:last-child{
            border-bottom:none;
          }
          .data_number{
            color:lightblue;
            font-size:22px;
            font-weight:bold;
            margin-left:5px;
            font-style:italic;
          }
        }
      }
    }
  }
  .guide_wrap{
    .introduce{
      li{
        position:relative;
        &:last-child{
          .guide_content{
            border-left:none!important;
          }
        }
        .title_name{
          margin-left:30px!important;
          position:relative;
          z-index:2;
          line-height:65px!important;
        }
        .guide_content{
          padding-left:60px!important;
          padding-right:60px!important;
          margin-top:-18px;
          position:relative;
          z-index:2;
        }
        &.list:after{
          content:'';
          clear:both;
          display:inline-block;
          width:calc(100% - 70px)!important;
          height:calc(100% - 15px)!important;
          background:rgba(27,31,59,.8);
          position:absolute;
          top:0;
          right:0;
          z-index:1;
          border-radius 6px;
        }
      }
    }
  }
}
