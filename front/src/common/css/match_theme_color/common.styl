.basic
  //自定义-表格样式
  .el-table{
    background:none;
    border:2px solid #6870b8;
  }
  .el-table th.is-leaf{
    background:none;
    text-align:center;
    border-bottom: 1px solid #4e537d;
    border-right:1px solid #4e537d;
    &:nth-last-child(2){
      border-right:none;
    }
    .cell{
      font-size:16px;
      font-weight:bold;
    }
  }
  .el-table--border th{
    border-color:#FFF;
    font-weight: normal;
  }
  .el-table,.el-table thead{
    color:#fff;
  }
  .el-table::before{
    background-color: rgba(0,0,0,0)!important;
  }
  .el-table--border::after, .el-table--group::after{
    display:none;
  }
  .el-table tr{
    background:rgba(0,0,0,0);
    td{
      padding:9px 0;
      border-bottom:1px solid #4e537d;
      border-right:1px solid #4e537d;
      &:last-child{
        border-right:none;
      }
    }
    &:last-child{
      td{
        border-bottom:none;
      }
    }
  }
  .first_line tr{
    &:nth-child(1){
      td{
        height:80px;
        line-height 80px;
        background:#2346a0;
      }
    }
  }
  .el-table--enable-row-hover .el-table__body tr:hover > td{
    background:#396494;
  }
  .pass,.no_pass{
    padding:3px 16px;
    display:inline-block;
    margin:2px;
    cursor:pointer;
  }
  .pass{
    border:1px solid lightblue;
    border-radius 3px;
    background: linear-gradient(rgba(0,0,0,0),#1742a8); /* 标准的语法 */
  }
  .no_pass{
    border:1px solid #f43940;
    border-radius 3px;
    background: linear-gradient(rgba(0,0,0,0),#892b43); /* 标准的语法 */
  }
