    vertical-align 0!important;
  }
}
.guide_title .title_index span{
  color:#152232!important;
}
.video_meng{
  width:100%;
  height:100%;
  position:fixed!important;
  .meng{
    width:100%;
    height:100%;
    background:rgba(34,4,93,.4);
    position:absolute;
    left:0;
    top:0;
    z-index:1;
  }
}
@media(max-width:1300px){
  .attack_view_list,.attack_match_info {
    .view_list,.table_wraps {
      height: 204px;
      li {
        font-size: 14px;
        line-height: 32px;
      }
    }
  }
  .platform_line{
    .rank{
      font-size:16px;
      line-height:50px;
      .data_weight{
        font-size:20px;
      }
    }
  }
  .task_list{
    margin-bottom:15px;
  }
  .task_list ul li{
    width: 178px;
    margin-right: 26px;
    height:265px;
    .status{
      padding-top:10px;
      font-size:14px;
      .now_score{
        font-size:24px!important;
      }
    }
  }
  .attack_match_info{
    .table_wraps{
      padding:18px;
      .tables{
        font-size:12px;
        div{
          line-height:22px!important;
        }
      }
    }
  }
  .task_list .min_7{
    height:265px;
  }
  .img_status{
    width:65px
  }
  .img_status2{
    width:55px
  }
}
