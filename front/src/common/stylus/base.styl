html
  width:100%
  height:100%
  min-width:1200px;
body,html
  padding:0
  margin:0
  font-family: 'pingFang SC','STHeitiSC-Light','Helvetica-Light',arial
*
  box-sizing: border-box

.form_parent_style .el-input__inner,.form_parent_style .el-textarea__inner{
  border:1px solid inputBorderColor;
  background-color:inputContentColor;
}
.form_parent_style .el-input.is-disabled .el-input__inner,.form_parent_style .el-input.is-disabled .el-input__inner{
  border:1px solid inputBorderColor;
  background-color:inputContentColor;
}
.form_parent_style .el-textarea.is-disabled .el-textarea__inner{
  border:1px solid inputBorderColor;
  background-color:inputContentColor;
}
  /*公用表格样式*/
#homepage{
  .el-checkbox__inner{
    background:rgba(0,0,0,0);
  }
  .el-textarea__inner, .el-input__inner {
    background: #0d223d!important;
    resize: none!important;
    border-color: #053a66!important;
    color: #ccc;
    font-family:'微软雅黑'
  }
  .ordinary .el-table--enable-row-hover .el-table__body tr:hover {
    -moz-background-size: 100% 97%;
    -webkit-background-size: 100% 97%;
  }
  .el-table th.is-leaf{
    border-bottom:none;
  }
  .ordinary .el-table td{
    border-bottom:none;
  }
  .ordinary thead tr{
    background:#102947!important;
    th{
      height:50px;
      line-height:50px;
      padding:0;
      background: #102947 !important;
      border-left:1px solid #2a4362!important;
      &:first-child{
        border-left:none;
      }
    }
  }
  .ordinary tbody tr{
    &:hover{
      cursor:pointer;
    }
  }
  .el-table__body-wrapper td {
    border: 1px solid #2a4362;
    background-color:none;
  }
  .el-table{
    background-color: rgba(0,0,0,0)!important;
  }
  .el-table--border{
    border:none;
  }
  .el-table tr{
    background-color:rgba(0,0,0,0);
  }
  //.ordinary .el-table--border tr td:last-child{
  //   border-right:1px solid #2a4362!important;
  // }
  //.ordinary .el-table tr:last-child td{
  //  border-bottom:1px solid #2a4362!important;
  //}

  .ordinary{
    border-right:1px solid #2a4362!important;
    border-bottom:1px solid #2a4362!important;
  }

  .ordinary .el-table .cell{
    color:#b8b9bb;
  }
  .ordinary .el-table thead th .cell{
    color:#fff;
    text-align:center
  }
  .ordinary .el-input-number{
    line-height :30px;
  }
  .ordinary .el-input-number__decrease, .ordinary .el-input-number__increase{
    color: #FFF;
    height: 28px
    border :none;
    background:#0d223d;
  }
  .ordinary .el-input-number__decrease{
    border-right:1px solid #053a66;
  }
  .ordinary .el-input-number__increase{
    border-left:1px solid #053a66;
  }
  .ordinary .el-input-number .el-input__inner{
    padding-left:50px;
  }
  .el-table--border::after, .el-table--group::after, .el-table::before {
    content: '';
    position: absolute;
    background-color: rgba(0,0,0,0);
    z-index: 1;
  }
  .el-table--border td, .el-table--border th, .el-table__body-wrapper .el-table--border.is-scrolling-left~.el-table__fixed {
    border-right: 1px solid rgba(0,0,0,0);
  }
  .el-table--enable-row-hover .el-table__body tr:hover {
    background-color: #0d233d;
    background-image: url('~@/assets/img/table2.png');
    background-repeat: no-repeat;
    background-position: 0 1px;
    background-size: 100% 97%;
    -moz-background-size: 100% 97%;
    -webkit-background-size: 100% 97%;
  }
  .el-table--enable-row-hover .el-table__body tr:hover>td{
    background-color:rgba(0,0,0,0);
  }
  .el-table--enable-row-hover .el-table__body tr:hover td .cell{
    color:#fff!important;
  }

  .noHover .el-table--enable-row-hover .el-table__body tr:hover{
    background :none;
  }
}
.form_parent_style
  text-align:left
  .form_title
    font-size:28px;
    line-height:50px;
    height:50px;
    border-bottom:1px solid inputBorderColor;
    text-align:center;
    color:white;
    margin-bottom:32px;
  .el-form-item__label
    color:darkblue2;
  .el-form-item__content
    color:white;
  .createSubmit
    match-confirmbtn-md()
    padding:0
  .cancelSubmit
    match-cancelbtn-md()
    padding:0
.def_btn
  match-confirmbtn-md()
  width:80px;
  height: 32px;
  line-height: 32px;
  padding:0
.def_btn:hover,.def_btn:focus,.def_btn.is-disabled,.def_btn.is-disabled:hover
  match-confirmbtn-md()
  width: 80px;
  height: 32px;
  line-height: 32px;
  padding:0
