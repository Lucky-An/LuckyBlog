.screen_handle{
  width:24px;
  height:24px;
  cursor:pointer;
  z-index:100;
  display:inline-block;
  .full_screen{
    width:100%;
    height:100%;
    background:url('~@/assets/img/ad5/attack/qp.png') no-repeat;
    background-size:100% 100%;
    display:inline-block;
  }
  .shou_screen{
    width:100%;
    height:100%;
    background:url('~@/assets/img/ad5/attack/shou.png') no-repeat;
    background-size:100% 100%;
    display:inline-block;
  }
