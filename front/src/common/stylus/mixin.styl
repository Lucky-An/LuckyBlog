match-cancelbtn-sm()
  btn-sm()
  match-cancel-btn-style()
confirm-btn-lg()
  btn-lg()
  btn-style()
confirm-btn-md()
  btn-md()
  btn-style()
confirm-btn-sm()
  btn-sm()
  btn-style()
edge-angle()
  background:url('~@/assets/img/bian.png') left top no-repeat,url('~@/assets/img/bian2.png') right top no-repeat,url('~@/assets/img/bian3.png') left bottom no-repeat,url('~@/assets/img/bian4.png') right bottom no-repeat
set_bg($name)
  background:url('~@/assets/img/'+$name)no-repeat;
  background-size:100% 100%;
