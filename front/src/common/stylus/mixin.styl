box-bg($size)
  background-image:url('~@/assets/img/box_'+$size+'.jpg')
  background-repeat: no-repeat
  -webkit-background-size: 100% 100%
  background-size: 100% 100%
btn-style()
  text-align:center
  color:white
  display:inline-block
  background:url('~@/assets/img/button/an.png')
  -webkit-background-size: 100% 100%
  background-size: 100% 100%
  border:none;
match-confirm-btn-style()
  text-align:center
  color:white
  display:inline-block
  background:url('~@/assets/img/button/list_btn.png')
  -webkit-background-size: 100% 100%
  background-size: 100% 100%
  border:none;
  border-radius:0
match-cancel-btn-style()
  text-align:center
  color:white
  display:inline-block
  background:url('~@/assets/img/button/disabled_list_btn.png')
  -webkit-background-size: 100% 100%
  background-size: 100% 100%
  border:none;
  border-radius:0
btn-lg()
  width:220px
  height:46px
  line-height:46px
  cursor:pointer;
btn-md()
  width:153px
  height:40px
  line-height:40px
btn-sm()
  width:110px
  height:28px
  line-height:28px
  cursor:pointer;
match-confirmbtn-lg()
  btn-lg()
  match-confirm-btn-style()
